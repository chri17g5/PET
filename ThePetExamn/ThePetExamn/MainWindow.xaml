<Window x:Class="ThePetExamn.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ThePetExamn"
        xmlns:view="clr-namespace:ThePetExamn.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="850">
    <Grid>
        <TabControl>
            <TabItem Header="Agents">
                <view:AgentsControl  x:Name="tabAgents"/>
            </TabItem>
            <TabItem Header="Informants">
                <view:AgentsControl  x:Name="tabInformants"/>
            </TabItem>
            <TabItem Header="BandeNavn">
                <view:AgentsControl  x:Name="tabGangs"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
