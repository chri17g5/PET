<Window x:Class="ThePetExamn.View.CreationView.CreateAgent"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ThePetExamn.View.CreationView"
        mc:Ignorable="d"
        Title="CreateAgent" Height="400" Width="300">
    <Grid>
        <StackPanel>
            <Label Content="Tilføj Agent" FontSize="25" FontWeight="Bold"/>
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="Navn:" Margin="5,0,5,0"/>
                <TextBox x:Name="FirstName" Width="200" BorderBrush="Black" BorderThickness="2"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
